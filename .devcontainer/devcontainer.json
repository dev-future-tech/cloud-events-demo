{
    "name" : "cloud-events-demo",
    "dockerComposeFile": "../docker-compose.yml",
    "service" : "rabbitmq",
    "forwardPorts": [5672,15672],
    "workspaceFolder": "/workspaces/cloud-events-demo",
    "features": {
        "ghcr.io/devcontainers/features/java:1": {
            "version": "25",
            "installMaven": "true",
            "installGradle": "false"
        },
        "ghcr.io/devcontainers/features/git:1": {}
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack",
                "praxai.mqexplorer",
                "ms-azuretools.vscode-container"
            ]
        }
    }
}
